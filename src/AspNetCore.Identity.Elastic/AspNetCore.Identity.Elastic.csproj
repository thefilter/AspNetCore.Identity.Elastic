<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.0.0</Version>
    <Authors>The Filter</Authors>
    <Company>The Filter</Company>
    <Description>Elasticsearch data store adaptor for ASP.NET Core Identity.</Description>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <Copyright>The Filter</Copyright>
    <PackageProjectUrl>https://github.com/thefilter/AspNetCore.Identity.Elastic</PackageProjectUrl>
    <RepositoryUrl></RepositoryUrl>
    <PackageIconUrl>https://d2li4vph18vuua.cloudfront.net/nuget/f-icon.png</PackageIconUrl>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageTags>identity, elasticsearch, elastic, aspnetcore, auth</PackageTags>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Identity" Version="2.0.0" />
    <PackageReference Include="NEST" Version="5.5.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>